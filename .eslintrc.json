{
	"extends": "next/core-web-vitals",
	"rules": {
		"@typescript-eslint/no-explicit-any": "off",
		"@typescript-eslint/no-unused-vars": "off",
		"@typescript-eslint/no-var-requires": "off",
		"@typescript-eslint/no-empty-object-types:": "off"
	},
	"ignorePatterns": [
		"node_modules/",
		".next/",
		"/src/generated",
		"prisma/generated/",
		"**/node_modules/.prisma/",
		"**/@prisma/client/"
	]
}
